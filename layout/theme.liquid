    var preload = document.querySelector('link[href="{{ 'theme.min.css' | asset_url }}"]');
    var stylesheet = loadCSS("{{ 'theme.min.css' | asset_url }}", preload);
    onloadCSS(stylesheet, function() { stylesheet.loaded = true; });
